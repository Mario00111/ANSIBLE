[DOMAIN]
# Type de domaine : WLS
type=WLS
domainName={{ inventory_hostname.split('.')[0] }}_domain
domainLocation=/u01/domains/
# Emplacement des applications Fusion Middleware (comme EM par exemple)
appLocation=/u01/applications

# Serveur d'administration
# NE PAS CHANGER le nom du serveur d'admin
adminServerName=AdminServer
adminUser=weblogic
adminPassword=weblogic1
adminServerAddress={{ inventory_hostname.split('.')[0] }}
adminServerPort=7001
adminServerSSLPort=7002

# Composant MAIL SESSION utilisé pour envoyer des emails
